@import "/css/color/color.wxss";

.modular.module-bottom-space{margin-bottom: 20rpx;}
.img,.scale-rule{width: 100%;height: 100%;}
.scale-rule{position: absolute;top: 0;left: 0;}


/***************************** 菜单链接 star *****************************/
/* 滑动菜单 */
.tpl-scroll-menu{white-space: nowrap;line-height: 92rpx;}
.scroll-menu-item{padding: 0 30rpx;display: inline-block;}
.scroll-menu-item.active{color: var(--mColor);font-weight: bold;}

/* 可滑动图片分类 */
/* .tpl-sliding-menu,.tpl-menu-list{background: #fff;} */
.tpl-sliding-menu .wx-swiper-dots.wx-swiper-dots-horizontal{height: 30rpx;bottom: 10rpx;}
.tpl-sliding-menu .wx-swiper-dot-active{background: var(--mColor);}
.tpl-menu-item-box{padding: 30rpx 0;box-sizing: border-box;}
.tpl-menu-item{display: inline-block;width: 25%;line-height: 100%;font-size: 26rpx;text-align: center;vertical-align: top;}
.tpl-menu-item:nth-child(n+5){margin-top: 20rpx;}
.tpl-menu-img{width: 100rpx;height: 100rpx;margin-bottom: 14rpx;}
/*--------------------------- 分类配置 ------------------------*/
.menu-two .tpl-menu-item{width: 50%;}
.menu-two .tpl-menu-item:nth-child(n+3){margin-top: 20rpx;}
.menu-two .tpl-menu-item:not(:nth-child(n+3)){margin-top: 0;}
.menu-three .tpl-menu-item{width: 33.33%;}
.menu-three .tpl-menu-item:nth-child(n+4){margin-top: 20rpx;}
.menu-three .tpl-menu-item:not(:nth-child(n+4)){margin-top: 0;}
.menu-five .tpl-menu-item{width: 20%;}
.menu-five .tpl-menu-item:nth-child(n+6){margin-top: 20rpx;}
.menu-five .tpl-menu-item:not(:nth-child(n+6)){margin-top: 0;}
.menu-filletS .tpl-menu-img{border-radius: 10%;}
.menu-filletM .tpl-menu-img{border-radius: 20%;}
.menu-filletL .tpl-menu-img{border-radius: 30%;}
.menu-filletXL .tpl-menu-img{border-radius: 40%;}
.menu-circle .tpl-menu-img{border-radius: 50%;}


/* 图片背景分类 */
.tpl-imgbg-menu{overflow: hidden;}
.imgbg-menu-item{width: 374rpx;height: 374rpx;position: relative;background: #000;color: #fff;float: left;}
.imgbg-menu-item:nth-child(2n+1){margin-right: 2rpx;}
.imgbg-menu-item:nth-child(n+3){margin-top: 2rpx;}
.imgbg-menuBg{width: 100%;height: 100%;position: absolute;top: 0;left: 0;opacity: 0.6;}
.imgbg-menu-txt{position: absolute;top: 0;left: 0;width: 100%;height: 100%;display: flex;align-items: center;justify-content: center;font-size: 36rpx;}

/* 栅格图片分类 */
.tpl-grid-menu{overflow: hidden;}
.grid-menu-item{--width:33.33%;}
.grid-menu-item{width: var(--width);padding-bottom: var(--width);position: relative;line-height: 100%;float: left;}
.grid-menu-item:not(:nth-child(-n+3))::before{content: '';display: block;width: 100%;height: 1px;background: #d3d3d3;transform: scaleY(0.5);position: absolute;top: 0;left: 0;} 
.grid-menu-item-cont{width: 100%;height: 100%;position: absolute;top: 0;left: 0;display: flex;align-items: center;justify-content: center;text-align: center;font-size: 28rpx;}
.grid-menu-img{width:100rpx;height: 100rpx;margin-bottom: 10rpx;}
.hid-menu-text .grid-menu-txt{display: none;}

/***************************** 菜单链接 end *****************************/

.tpl-img-single{width: 100%;}



/***************************** 自定义图片 star *****************************/
.img-module{-webkit-column-gap:0;column-gap:0;}
.img-module::after{content: '';display: block;clear: both;visibility: hidden;}
.img-module .img-module-img{float: left;width: 100%;position: relative;-webkit-column-break-inside:avoid;}
.img-module-ad2th{-webkit-column-count:2;column-count:2;}
.img-module-ad2th .img-module-img{padding-top: 100%;}
.img-module-ad3th{-webkit-column-count:3;column-count:3;}
.img-module-ad3th .img-module-img{padding-top: 120%;}
.img-module-ad3th2 .img-module-img:first-child{width:46%;padding-top: 60%;}
.img-module-ad3th2 .img-module-img:nth-child(n+2){width:54%;padding-top: 30%;}
.img-module-ad4th{-webkit-column-count:4;column-count:4;}
.img-module-ad4th .img-module-img{padding-top: 140%;} 
.img-module-ad4th2{-webkit-column-count:2;column-count:2;}
.img-module-ad4th2 .img-module-img{padding-top: 100%;}
.img-module-ad4th3 .img-module-img:first-child{width:40%;padding-top: 60%;}
.img-module-ad4th3 .img-module-img:nth-child(2){width:60%;padding-top: 30%;}
.img-module-ad4th3 .img-module-img:nth-child(3),.img-module-ad4th3 .img-module-img:nth-child(4){width:30%;padding-top: 30%;}
.img-module-ad5th .img-module-img:first-child{width: 100%;padding-top:50%;}
.img-module-ad5th .img-module-img:nth-child(2),.img-module-ad5th .img-module-img:nth-child(5){width: 50%;padding-top:48%;}
.img-module-ad5th .img-module-img:nth-child(3),.img-module-ad5th .img-module-img:nth-child(4){width: 50%;padding-top:52%;}
.img-module-ad5th .img-module-img:nth-child(4){margin-top: -4%;} 
/* 图片模块内间距 */
.img-module{--adver-space:0px;}
.img-module.img-space-mini{--adver-space:2rpx;}
.img-module.img-space-large{--adver-space:16rpx;}
.img-module{-webkit-column-gap:var(--adver-space);column-gap:var(--adver-space);}
.img-module.img-module-ad3th2 .img-module-img:nth-child(n+2){margin-left: var(--adver-space);width:calc(54% - var(--adver-space));padding-top: calc(30% - (var(--adver-space)/2));}
.img-module.img-module-ad3th2 .img-module-img:nth-child(n+3){margin-top: var(--adver-space);}
.img-module.img-module-ad4th2 .img-module-img:nth-child(2n){margin-top: var(--adver-space);} 
.img-module.img-module-ad4th3 .img-module-img:first-child{width: calc(40% - var(--adver-space));} 
.img-module.img-module-ad4th3 .img-module-img:nth-child(n+2){margin-left: var(--adver-space);padding-top: calc(30% - (var(--adver-space)/2));} 
.img-module.img-module-ad4th3 .img-module-img:nth-child(n+3){margin-top: var(--adver-space);width:calc(30% - (var(--adver-space)/2));}
.img-module.img-module-ad5th .img-module-img:nth-child(n+2){width: calc(50% - (var(--adver-space)/2));}
.img-module.img-module-ad5th .img-module-img:nth-child(2),.img-module.img-module-ad5th .img-module-img:nth-child(4){margin-right: var(--adver-space);}
.img-module.img-module-ad5th .img-module-img:nth-child(2),.img-module.img-module-ad5th .img-module-img:nth-child(3),.img-module.img-module-ad5th .img-module-img:nth-child(5){margin-top: var(--adver-space);}
.img-module.img-module-ad5th .img-module-img:nth-child(4){margin-top: calc(var(--adver-space) - 4%);} 
/***************************** 自定义图片 end *****************************/



.tpl-swiper-box{width:100%;padding-bottom: 50%;position: relative;}
.tpl-swiper{width:100%;height: 100%;position: absolute;top: 0;left: 0;}
.tpl-swiper .wx-swiper-dots{height: 36rpx;bottom: 0;padding-right: 4rpx;}

/* 扁平条 */
.dots-flat .wx-swiper-dot{width: 50rpx;height: 8rpx;border-radius: 20rpx;}
.dots-flat .wx-swiper-dot:not(:last-child){margin-right: 10rpx;}

.dots-num2 .wx-swiper-dots{display: none;}
.dots-num2 .num-dots{position: absolute;bottom: 10rpx;left: 50%;transform: translateX(-50%);text-align: center;}
.dots-num2 .num-dot{display: inline-block;width: 30rpx;height: 30rpx;line-height: 30rpx;border-radius: 50%;background: #ccc;font-size: 20rpx;}
.dots-num2 .num-dot:not(:last-child){margin-right: 10rpx;}
.dots-num2 .num-dot.active{background: #1ca27e;color: #fff;}

/* 数字 */
.dots-num .wx-swiper-dots{display: none;} 
.dots-num .num-dots{position: absolute;bottom: 0;right: 0;padding: 6rpx 20rpx;background: rgba(0, 0, 0, 0.2);color: #fff;padding-left: 30rpx;border-radius: 60rpx 0 0 60rpx;}

/* 进度 */
.dots-progress .wx-swiper-dots{display: none;} 
.dots-progress .progress-dots{width:100%;position: absolute;bottom: 0;left: 0;height: 6rpx;background: rgba(0, 0, 0, 0.25);}
.dots-progress .progress-dot{height: 100%;position: absolute;left: 0;top: 0;background: #f66;}

.imgContSwiper .swiper{height: 1600rpx!important;}
.imgContSwiper swiper-item{width:40%!important;}
.imgContSwiper .swiper-item{margin: 0 10rpx;}
.imgContSwiper .swiper-item-img{width: 100%;padding-bottom: 120%;position: relative;} 



.videoTemBox{width: 100%;padding-bottom: 56.25%;position: relative;}
.videoTemBox video,.video-cover{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.videoTop{width: 100%;height: 80rpx;background-image: linear-gradient( rgba(0,0,0,0.5) , rgba(0,0,0,0));color: #fff;display: flex;display: -webkit-flex;align-items: center;justify-content: space-between;}
.videoBack{width: 80rpx;height: 80rpx;text-align: center;line-height: 80rpx;background: rgba(255, 255, 255, 0.5);}


/* 热区 */
.hot-region-img{width: 100%;}
.hot-region{position: relative;overflow: hidden;}
.hot-region-item{position: absolute;max-width: 100%;max-height: 100%;}

/* 地图 */
.tpl-map-box{width:100%;height: 400rpx;position: relative;}
.tpl-map-cont{width: 100%;height: 100%;position: absolute;top: 0;left: 0;}

/* 文本 */
.tpl-text{line-height:1.5;white-space: pre-wrap;}


/****************** 浮动按钮 star *******************/
.asideFloatBox{position: fixed;right: 30rpx;bottom: 30rpx;width: 80rpx;z-index: 10;}
.asideFloatItem{width: 80rpx;height: 80rpx;background: transparent;padding: 0;border-radius: inherit;line-height: inherit;}
.asideFloatItem:not(:last-child){margin-bottom: 10rpx;}
.asideFloatItem .asideFloatTxt{display: flex;align-items: center;justify-content: center;background: var(--mColor);color: #fff;font-size: 28rpx;border-radius: 50%;}
/****************** 浮动按钮 end *******************/