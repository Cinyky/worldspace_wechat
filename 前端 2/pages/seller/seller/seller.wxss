.header {
	position: fixed;
	display: -webkit-flex;
	left: 0;
	top: 0;
	width: 100%;
	padding: 20rpx;
	box-sizing: border-box;
	z-index: 1;
	-webkit-transition: background 0.5s ease-in-out;
}
.header.scrolled {
	background: #e95a7a;
}
.header .icon {
	width: 40rpx;
	height: 40rpx;
    display: inline-block;
	vertical-align:middle;
}
.header .location {
	color: #fff;
    font-size: 28rpx;
	line-height: 40rpx;
	min-width: 100rpx;
	max-width: 340rpx;
	padding: 8rpx 20rpx 8rpx 10rpx;
	border-radius: 30rpx;
	background: rgba(0,0,0,0.3);
	margin-right: 20rpx;
	vertical-align: middle;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.header .search {
-webkit-flex: 1;
	color: #888;
    background-color:rgba(255, 255, 255, 0.3);
	font-size: 28rpx;
	border-radius: 30rpx;
    padding: 0 20rpx;
}
.header .search input{
    display: inline-block;
    height:56rpx;
    text-overflow:clip;
    overflow:hidden;
    white-space:nowrap;
    font-family:UICTFontTextStyleBody;
    min-height:56rpx;
    width: 80%;
    vertical-align: middle;
    margin-left: 10rpx;
}
