/*搜索框*/
page{
    background-color: #fff;
}
.searchbox{
    padding: 20rpx;
    background-color: #fff;
    position: relative;
    font-size: 24rpx;
}
.searchbox .address{
    width: 160rpx;
    background-color: #f7f7f7;
    line-height: 36rpx;
    padding: 16rpx 0;
    padding-right: 6rpx;
    float: left;
    color: #666;
    margin-right: 10rpx;
    border-radius: 3px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.address image{
    width: 36rpx;
    height: 36rpx;
    float: left;
    margin: 0 6rpx;
}
.searchbox .inputbox{
    float: left;
    padding: 6rpx 0;
    border: 2rpx solid #f2f2f2;
    border-radius: 3px;
    position: relative;
    padding-right: 12rpx;
    width: 518rpx;
}

.searchbox .inputbox image{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 12rpx;
    left: 12rpx;
}
.searchbox .inputbox input{
    line-height: 1.4rem;
    height: 1.4rem;
    min-height:1.4rem;
    margin-left: 64rpx;
    display: block;
    font-size: 24rpx;
}


/*标题头*/
.header {
	background-color: #f7f7f7;
	font-size: 28rpx;
	padding: 20rpx;
	line-height: 40rpx;
	color: #444;
}

/*当前定位*/
.current {
	color: #e95a7a;
	padding: 20rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.current text {
	font-size: 28rpx;
	width: 100%;
}

.current image {
	width: 40rpx;
	height: 40rpx;
}

.current image:after {
	content: '重新定位';
}

/*结果行*/
.item {
	padding: 20rpx;
	display: flex;
	flex-direction: column;
	border-bottom: 1px solid #eee;
}

.item .title {
	width: 100%;
	font-size: 32rpx;
	line-height: 60rpx;
	color: #333;
}

.item .address {
	width: 100%;
	font-size: 28rpx;
	line-height: 48rpx;
	color: #999;
}
.list{
    background-color: #fff;
}
.postop{
    position: absolute;
    top: 110rpx;
    width: 100%;
    background-color: #fff;
}